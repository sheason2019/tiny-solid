{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "es2016",
    "module": "ES2015",
    "moduleResolution": "node",
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "jsxImportSource": "tiny-solid",
    "skipLibCheck": true
  },
  "include": ["src"]
}
